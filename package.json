    "mongodb": "^6.16.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mcp",
    "cursor",
    "claude",
    "backend",
    "documentation",
    "erd",
    "swagger",
    "mongodb",
